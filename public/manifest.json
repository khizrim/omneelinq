{
  "name": "Omneelinq",
  "description": "Chrome extension for opening multiple links at once",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Link it up!"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "/images/icon-16.png",
    "32": "/images/icon-32.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
